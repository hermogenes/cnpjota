{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "0.1.22",
  "name": "cnpjota",
  "type": "module",
  "repository": {
    "url": "https://github.com/hermogenes/cnpjota"
  },
  "private": false,
  "files": [
    "README.md",
    "LICENSE",
    "bin",
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "bun run --conditions=development ./src/index.ts",
    "generate:types": "tsc --emitDeclarationOnly --outDir dist",
    "prebuild": "rimraf dist",
    "build": "bun build src/index.ts --target node --outdir dist --sourcemap",
    "format": "prettier --write src/*",
    "prepublishOnly": "bun run build"
  },
  "bin": {
    "cnpjota": "./bin/cnpjota"
  },
  "devDependencies": {
    "@types/bun": "1.2.20",
    "@tsconfig/bun": "1.0.8",
    "rimraf": "^6.0.1",
    "shx": "^0.3.4",
    "prettier": "3.5.3",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "commander": "^14.0.0",
    "table": "^6.9.0",
    "zod": "3.25.76"
  },
  "prettier": {
    "semi": false,
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": false
  },
  "packageManager": "bun@1.2.20"
}